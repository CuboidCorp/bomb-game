  m_TableData:
  - m_Id: 33936601088
    m_Localized: Introduction
    m_Metadata:
      m_Items: []
  - m_Id: 222474760192
    m_Localized: This module is made of {0} to {1} colored wires.
    m_Metadata:
      m_Items:
      - rid: 4501475455923650834
  - m_Id: 265327964160
    m_Localized: This module is made of a labyrinth made of a grid of {0}x{1} squares.
      The objective is to help the agent move the cursor from the beginning of the
      labyrinth (Green square) to the end of the labyrinth (Blue squares) without
      going through the walls that the agent can not see. The current position in
      the labyrinth is shown through a blinking yellow square.
    m_Metadata:
      m_Items:
      - rid: 4501475455923650834
  - m_Id: 2960232460808192
    m_Localized: Release after {0:1 second|{} seconds}
    m_Metadata:
      m_Items:
      - rid: 4501475455923650834
  - m_Id: 2961313454583808
    m_Localized: Release immediatly
    m_Metadata:
      m_Items: []
  - m_Id: 2961590408671232
    m_Localized: Release when timer contains a {0}
    m_Metadata:
      m_Items:
      - rid: 4501475455923650834
  - m_Id: 2961743685316608
    m_Localized: Release when seconds are between {0} and {1}
    m_Metadata:
      m_Items:
      - rid: 4501475455923650834
